{
  "name": "ng-x-rocket",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod",
    "start": "npm run env -s && ng serve --aot --proxy-config proxy.conf.js",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4202",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --code-coverage --watch=false",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "generate": "ng generate"
  },
  "dependencies": {
    "@agm/core": "^1.0.0-beta.7",
    "@angular/animation": "^4.0.0-beta.8",
    "@angular/common": "^6.1.10",
    "@angular/core": "^6.1.10",
    "@angular/forms": "^6.1.10",
    "@angular/http": "^6.1.10",
    "@angular/platform-browser": "^6.1.10",
    "@angular/platform-browser-dynamic": "^6.1.10",
    "@angular/router": "^6.1.10",
    "@angular/service-worker": "^6.1.10",
    "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
    "@ng-bootstrap/ng-bootstrap": "^2.0.0",
    "@ngx-translate/core": "^10.0.1",
    "@types/babel-types": "^6.25.1",
    "@types/jquery": "^3.3.29",
    "@types/jspdf": "^1.2.2",
    "angular-6-datatable": "^0.8.0",
    "angular2-csv": "^0.2.3",
    "angular2-recaptcha": "^1.1.0",
    "angularx-qrcode": "^1.5.3",
    "bootstrap": "^5.1.3",
    "core-js": "^2.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "enhanced-resolve": "^3.3.0",
    "express": "^4.18.1",
    "http-server": "^0.11.1",
    "jspdf": "^1.5.3",
    "jspdf-autotable": "^2.3.5",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.11",
    "ng-click-outside": "^4.0.0",
    "ng-recaptcha": "^5.0.0",
    "ng-socket-io": "^0.2.4",
    "ng2-file-upload": "^1.3.0",
    "ng2-order-pipe": "^0.1.5",
    "ng4-popover": "^1.0.23",
    "ngx-bootstrap": "^4.0.0",
    "ngx-extended-pdf-viewer": "^2.0.0-alpha.2",
    "ngx-loading": "^2.0.1",
    "ngx-pagination": "^3.2.1",
    "node-sass": "^7.0.1",
    "npx": "^10.2.0",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "socket.io": "^2.1.1",
    "sweetalert": "^2.1.2",
    "sweetalert2": "^7.33.1",
    "zone.js": "^0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.7.5",
